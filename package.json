{
  "name": "yaras",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^5.0.1",
    "font-awesome": "^4.7.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "reset-css": "^2.2.0",
    "susy": "^2.2.12"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "chai": "^4.0.2",
    "chai-enzyme": "^0.7.1",
    "dirty-chai": "^2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-sort-imports-es6": "^0.0.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.4.2",
    "node-sass-chokidar": "^0.0.3",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "1.0.7",
    "sinon": "^2.3.4",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.5.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint-scss": "`npm bin`/stylelint --config=stylelint.config.js 'src/**/*.scss' --syntax scss",
    "lint-js": "`npm bin`/eslint --config=.eslintrc.js .",
    "start-js": "cross-env NODE_PATH=./src react-scripts start",
    "start": "run-p watch-css start-js",
    "build": "yarn run build-css && cross-env NODE_PATH=./src react-scripts build",
    "test-js": "cross-env NODE_ENV=development NODE_PATH=./src mocha --require ignore-styles --require jsdom-global/register --compilers js:babel-core/register src/**/*.test.js",
    "test": "yarn run lint-scss && yarn run lint-js && yarn run test-js",
    "eject": "react-scripts eject"
  }
}
